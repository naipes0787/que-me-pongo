{{#partial "contenido"}}

<div>
    <h2>
        Guardarropas: {{ nombre }}
    </h2>

    {{#each prendas}}
    <div class="prenda">
        <img src="data:image/png;base64, {{foto}}" alt="{{ nombre }}" />
        <div class="detalles">
            <h2>{{nombre}}</h2>
            Color: {{ colorPrincipal }}
        </div>
    </div>
    {{/each}}
</div>

{{/partial}}

{{>layout.hbs}}